<div class="center-container"> <!-- View dashboard --> 
    <div class="dash-title">
        <h4>My Dashboard</h4>
    </div>
</div>
<div class="main-container"> 
    <h2 class="dash-p-c">My Posts</h2>
    {{#each posts as |post| }} <!-- Displays all user posts -->
    {{#each posts}}
    <div class="btn-container">
        <a href="/dash/update/{{id}}" class="waves-effect waves-light btn-small grey lighten-4 black-text">Edit</a>
    </div>
    {{> view-posts-partials }}  
    {{/each}}
    {{/each}}

    <h2 class="dash-p-c">My Comments</h2>
    {{#each posts as |post| }} <!-- Displays all user comments -->
    {{#each comments as |comment|}}
    {{> view-my-comments-partials }}   
    {{/each}}
    {{/each}}
    <div class='btn-container'>
        <a href="/dash/create" class="postBtn"><i class="material-icons left">add</i>New Post</a>  
    </div>
</div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src = "https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="/js/global.js"></script>

    






