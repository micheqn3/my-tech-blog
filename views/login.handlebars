<div class="main-container"> <!-- Log in screen -->
        <div class="row">
            <div class="col l3 s12"></div>
            <div class="col l6 s12">
                <div class="card z-depth-3">
                    <div class="center log-sign-title">
                        <span >Log in</span>
                    </div>  
                    <form class="card-content form-container">
                        <div class="row">
                            <div class="input-field col s12">
                                <span class="fas fa-user prefix"></span>
                                <input type="text" id="username">
                                <label>Username</label>
                            </div>
                            <div class="input-field col s12">
                                <span class="fas fa-unlock prefix"></span>
                                <input type="text" id="password">
                                <label>Password</label>
                            </div>
                            <div class="col s12 center-align">
                                <button id="logInBtn"class="btn waves-effect waves-light align-center green-btn" type="submit">Log In</button>
                                <a class="a-link" href="/signup">Sign up here!</a>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
    </div>
</div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src = "https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="/js/login.js"></script>